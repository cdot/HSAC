{
  "name": "hsac",
  "version": "1.0.0",
  "license": "MIT",
  "description": "HSAC Sheds",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdot/HSAC.git"
  },
  "author": "Crawford Currie",
  "bugs": {
    "url": "https://github.com/cdot/HSAC/issues"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "homepage": "https://github.com/cdot/HSAC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "shared-node-browser": true,
      "jquery": true
    },
    "globals": {
      "define": false
    },
    "rules": {
      "strict": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "camelcase": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "no-debugger": 0,
      "no-alert": 1,
      "no-new": 0,
      "new-cap": 0,
      "dot-notation": 1,
      "no-trailing-spaces": 0,
      "no-useless-escape": 1,
      "no-use-before-define": 1,
      "no-loop-func": 1,
      "no-eval": 1,
      "quotes": 0
    }
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "clean-css": "^5.3.0",
    "html-minifier": "^4.0.0",
    "jquery-ui-touch-punch": "^0.2.3",
    "markdown-it": "^13.0.1",
    "mocha": "^10.0.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "fs-extra": "^10.1.0",
    "icon-font": "^0.0.1",
    "jquery": "^3.6.0",
    "jquery-confirm": "^3.3.4",
    "jquery-csv": "^1.0.21",
    "jquery-ui": "^1.13.1",
    "jquery-ui-dist": "^1.13.1",
    "jquery-ui-iconfont": "^2.3.2",
    "jquery-validation": "^1.19.3",
    "jquery.toc": "^0.4.0",
    "js-cookie": "^3.0.1",
    "markdown-it": "^12.3.2",
    "requirejs": "^2.3.6",
    "requirejs-text": "^2.0.16",
    "tablesorter": "^2.31.3"
  }
}
