{
  "name": "hsac",
  "version": "1.0.0",
  "license": "MIT",
  "description": "HSAC Sheds",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdot/HSAC.git"
  },
  "author": "Crawford Currie",
  "bugs": {
    "url": "https://github.com/cdot/HSAC/issues"
  },
  "homepage": "https://github.com/cdot/HSAC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "shared-node-browser": true,
      "jquery": true
    },
    "globals": {
      "define": false
    },
    "rules": {
      "strict": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "camelcase": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "no-debugger": 0,
      "no-alert": 1,
      "no-new": 0,
      "new-cap": 0,
      "dot-notation": 1,
      "no-trailing-spaces": 0,
      "no-useless-escape": 1,
      "no-use-before-define": 1,
      "no-loop-func": 1,
      "no-eval": 1,
      "quotes": 0
    }
  },
  "devDependencies": {
    "clean-css": "^4.2.1",
    "fs-extra": "^8.1.0",
    "html-minifier": "^4.0.0",
    "request": "^2.88.0"
  },
  "dependencies": {
    "jquery-ui-iconfont": "^2.3.2"
  }
}
